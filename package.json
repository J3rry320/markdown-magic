{
  "name": "markdown-magic",
  "version": "1.1.2",
  "description": "A lightweight and configurable Markdown renderer for React and Next.js with syntax highlighting and table support.",
  "keywords": [
    "markdown",
    "parser",
    "react",
    "react-markdown",
    "tables",

    "markdown-to-html",
    "markdown-to-react",
   
    "syntax-highlighting",
    "renderer",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/J3rry320/markdown-magic.git"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/lib.umd.js",
  "module": "./dist/lib.module.js",
  "files": [
    "dist/"
  ],
  "source": "src/index.ts",
  "scripts": {
    "build": "microbundle --jsx React.createElement --jsxFragment React.Fragment --jsxImportSource react",
    "dev": "microbundle watch"
